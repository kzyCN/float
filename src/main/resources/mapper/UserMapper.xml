<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTO Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fsyume.dao.UserDao">

    <!--查询所有用户-->
    <select id="findAllUser" resultType="com.fsyume.eneity.User">
        select *
        from `user`
    </select>

    <!--通过用户名查询-->
    <select id="getUsernameByName" parameterType="com.fsyume.eneity.User" resultType="com.fsyume.eneity.User">
        select *
        from `user`
        where username = #{username}
    </select>


</mapper>